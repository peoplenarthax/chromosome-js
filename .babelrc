{
    "presets": [
        [
            "env",
            {
                "targets":{
                    "node": "6.10"
                }
            }
        ],
        "stage-3",
        "@babel/typescript"
    ],
    "env": {
        "test": {
            "presets": [
                [
                    "env",
                    {
                        "targets":{
                            "node": "current"
                        }
                    }
                ],
                "stage-3",
                "@babel/typescript"
            ],
            "plugins": [
                "transform-es2015-modules-commonjs",
                "@babel/proposal-class-properties",
                "@babel/proposal-object-rest-spread"
            ]
        }
    }
}
